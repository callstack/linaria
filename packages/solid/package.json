{
  "name": "@linaria/solid",
  "description": "Blazing fast zero-runtime CSS in JS library for SolidJS",
  "version": "4.0.0",
  "bugs": "https://github.com/callstack/linaria/issues",
  "contributors": [
    {
      "name": "Kirill Agalakov",
      "email": "raveclassic@gmail.com",
      "url": "https://github.com/raveclassic"
    }
  ],
  "dependencies": {
    "@linaria/core": "workspace:^",
    "@linaria/tags": "workspace:^"
  },
  "devDependencies": {
  },
  "engines": {
    "node": "^12.16.0 || >=13.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./esm/index.js",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./types/*.d.ts",
      "import": "./esm/*.js",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "esm/",
    "lib/",
    "processors/",
    "types/",
    "linaria.config.js"
  ],
  "homepage": "https://github.com/callstack/linaria#readme",
  "keywords": [
    "css",
    "css-in-js",
    "linaria",
    "solid",
    "styled-components"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:callstack/linaria.git",
  "scripts": {
  },
  "sideEffects": false,
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "processors/*": [
        "./types/processors/*.d.ts"
      ]
    }
  }
}
